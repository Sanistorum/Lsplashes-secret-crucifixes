local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
local Window = Library.CreateLib("Lsplashes crucifixes (balls)", "Midnight")

local Main = Window:NewTab("crucifix")

local MainSection = Main:NewSection("crucifix")

MainSection:NewKeyBind("Toggle UI", "Toggles the UI.", Enum.KeyCode.K, function()
        Library:ToggleUI()
end)

MainSection:NewButton("Figure Crucifix", "goofy demogorgan crucifix", function()
loadstring(game:HttpGet("https://pastebin.com/raw/yPCLyNaZ"))()
end)


MainSection:NewButton("Regular Crucifix", "best one credit to zepsyy", function()
loadstring(game:HttpGet('https://raw.githubusercontent.com/ZepsyyCodesLUA/Utilities/main/Doors/Crucifix.lua'))()
end)


MainSection:NewButton("Lightning Crucifix", "light", function()
loadstring(game:HttpGet("https://pastebin.com/raw/ZJGqyKJk"))()
end)


MainSection:NewButton("Spogebob Crucifix", "SPOGBOB11!!!!", function()
loadstring(game:HttpGet("https://pastebin.com/raw/aBKM8KuQ"))()
end)


MainSection:NewButton("Halloween Crucifix", "pumpkin.exe", function()
loadstring(game:HttpGet("https://raw.githubusercontent.com/Mye123/MyeWareHub/main/Halloween%20Crucifix"))()
end)
